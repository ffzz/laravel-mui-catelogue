{
    "private": true,
    "type": "module",
    "scripts": {
        "preinstall": "npx only-allow pnpm",
        "build": "vite build",
        "build:ssr": "vite build && vite build --ssr",
        "dev": "vite",
        "commitlint": "commitlint --edit",
        "format": "prettier --write \"**/*.{js,jsx,ts,tsx,css,md,json}\"",
        "format:check": "prettier --check resources/",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "types": "tsc --noEmit",
        "prepare": "husky",
        "phpstan": "vendor/bin/phpstan analyse",
        "phpstan:baseline": "vendor/bin/phpstan analyse --generate-baseline --allow-empty-baseline",
        "phpstan:clear-baseline": "rm -f phpstan-baseline.neon",
        "test": "vitest run",
        "test:watch": "vitest",
        "test:coverage": "vitest run --coverage",
        "test:backend": "php artisan test"
    },
    "devDependencies": {
        "@commitlint/cli": "^19.8.0",
        "@commitlint/config-conventional": "^19.8.0",
        "@eslint/js": "^9.23.0",
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/react": "^16.2.0",
        "eslint": "^9.23.0",
        "eslint-config-prettier": "^10.1.1",
        "eslint-plugin-react": "^7.37.4",
        "eslint-plugin-react-hooks": "^5.2.0",
        "husky": "^9.1.7",
        "lint-staged": "^15.5.0",
        "prettier": "^3.5.3",
        "prettier-plugin-organize-imports": "^4.1.0",
        "prettier-plugin-tailwindcss": "^0.6.11",
        "typescript-eslint": "^8.28.0",
        "vite-tsconfig-paths": "^5.1.4",
        "vitest": "^3.0.9"
    },
    "dependencies": {
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.0",
        "@fontsource/roboto": "^5.2.5",
        "@inertiajs/react": "^2.0.5",
        "@mui/icons-material": "^7.0.1",
        "@mui/material": "^7.0.1",
        "@tailwindcss/vite": "^4.0.17",
        "@tanstack/eslint-plugin-query": "^5.68.0",
        "@tanstack/react-query": "^5.70.0",
        "@tanstack/react-query-devtools": "^5.70.0",
        "@types/node": "^22.13.14",
        "@types/react": "^19.0.12",
        "@types/react-dom": "^19.0.4",
        "@vitejs/plugin-react": "^4.3.4",
        "axios": "^1.8.4",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "concurrently": "^9.1.2",
        "globals": "^16.0.0",
        "jsdom": "^26.0.0",
        "laravel-vite-plugin": "^1.2.0",
        "lucide-react": "^0.485.0",
        "react": "19.1.0",
        "react-dom": "19.1.0",
        "react-error-boundary": "^5.0.0",
        "tailwind-merge": "^3.0.2",
        "tailwindcss": "^4.0.17",
        "tailwindcss-animate": "^1.0.7",
        "typescript": "^5.8.2",
        "vite": "^6.2.3"
    },
    "peerDependencies": {
        "react": "^17.0.0 || ^18.0.0 || ^19.0.0",
        "react-dom": "^17.0.0 || ^18.0.0 || ^19.0.0"
    },
    "optionalDependencies": {
        "@rollup/rollup-linux-x64-gnu": "4.38.0",
        "@tailwindcss/oxide-linux-x64-gnu": "^4.0.17",
        "lightningcss-linux-x64-gnu": "^1.29.3"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "eslint --fix",
            "prettier --write"
        ],
        "*.{css,md,json}": [
            "prettier --write"
        ]
    },
    "packageManager": "pnpm@10.7.0",
    "engines": {
        "pnpm": ">=8.0.0",
        "node": ">=20"
    },
    "pnpm": {
        "overrides": {
            "lodash.isequal": "npm:lodash@^4.17.21"
        }
    }
}
